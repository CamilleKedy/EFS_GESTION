	<queryString language="SQL">
		<![CDATA[SELECT d.*
FROM donneur d
	inner join rdv r ON 
	 d.id_donneur = r.id_donneur 
	inner join collecte c ON 
	 r.id_collecte = c.id_collecte 
WHERE 
	 c.id_site = 1]]>
	</queryString>
